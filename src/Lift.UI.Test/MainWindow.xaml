<liftui:Window
    x:Class="Lift.UI.Test.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:liftui="https://github.com/pchuan98/Lift.UI"
    xmlns:local="clr-namespace:Lift.UI.Test"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    SystemBackdropType="Tabbed"
    mc:Ignorable="d">
    <StackPanel Orientation="Vertical">
        <Button Width="300" Height="100 " />

        <TextBox Margin="10" />

        <liftui:RangeSlider />

        <TextBlock Text="{x:Static liftui:LangKeys.Clear}" />
    </StackPanel>
</liftui:Window>
