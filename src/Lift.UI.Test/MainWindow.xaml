<liftui:Window
    x:Class="Lift.UI.Test.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:liftui="https://github.com/pchuan98/Lift.UI"
    xmlns:local="clr-namespace:Lift.UI.Test"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:propertyGrid="clr-namespace:Lift.UI.Test.Controls.PropertyGrid"
    Title="MainWindow"
    Width="800"
    Height="850"
    SystemBackdropType="Tabbed"
    Topmost="True"
    mc:Ignorable="d">
    <UniformGrid Rows="2">
        <liftui:PropertyGrid x:Name="LiftUiPropertyGrid" Description="hello" />
        <propertyGrid:PropertyGrid x:Name="PdPropertyGrid" Style="{StaticResource PropertyGrid.Base}" />
    </UniformGrid>
</liftui:Window>
